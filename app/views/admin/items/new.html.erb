<div class="container">
  <div class="row">
      <p class="h5 text-left bg-light text-dark px-3 my-3 ml-5">商品新規登録</p>
  </div>

  <%= form_with model: @item, url: admin_items_path, method: :post do |f| %>
  <%= render 'form', f: f, genres: @genres %>
    <div class="col-2 mx-auto">
    <div class="row form-group">
      <%= f.submit '新規登録', class:"btn btn-success btn-lg" %>
    </div>
    </div>
    <% end %>
  </div>
</div>